OUTPUT_FORMAT("elf32-sh")
SECTIONS {
	SLSTART 0x06004000 :
	{
		__ssStart = .;
		*( SLSTART )
	}
	.text ALIGN( 0x20 ) :
	{
		*( .text )
		*( .strings )
		__etext = .;
	}
	SLPROG ALIGN( 0x20 ) :
	{
		__slprog_start = .;
		*( SLPROG )
		__slprog_end = .;
	}
	.tors ALIGN( 0x10 ) :
	{
		___ctors = .;
		*( .ctors )
		___ctors_end = .;
		___dtors = .;
		*( .dtors )
		___dtors_end = .;
	}
	.data ALIGN( 0x10 ):
	{
		*( .data )
		__edata = .;
	}
	.rodate ALIGN( 0x10 ):
	{
		*( .rodata )
	}
	.bss ALIGN( 0x10 ) ( NOLOAD ):
	{
		__bss_start = .;
		*( .bss )
		*( COMMON )
		__bss_end = .;
	}
	.comment ALIGN( 0x10 ) ( NOLOAD ):
	{
		_end = .;
	}
}
